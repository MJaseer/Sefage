                    <!-- Page Heading -->
                    <h1 class="h3 mb-2 text-gray-800">Cars</h1>
                    <p class="mb-4">List of All Cars <a target="_blank" href="">vg</a>.</p>

                    <!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <div class="card-header py-3  bg-dark">
                            <h6 class="m-0 font-weight-bold text-light">Cars List</h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered text-dark" id="dataTable" width="100%"
                                    cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Image</th>
                                            <th>Name</th>
                                            <th>Brand</th>
                                            <th>Price</th>
                                            <th>Tickets</th>
                                            <th>Rate/Ticket</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>
                                            <th>#</th>
                                            <th>Image</th>
                                            <th>Name</th>
                                            <th>Brand</th>
                                            <th>Price</th>
                                            <th>Tickets</th>
                                            <th>Ticket Rate</th>
                                            <th>Action</th>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                        <%var i=1%>
                                            <% cars.forEach(function(car){ %>

                                                <tr>
                                                    <th scope="row">
                                                        <%= i%>
                                                    </th>
                                                    <td class="d-flex flex-raw">
                                                        <% for (var i = 0; i < 3 && i < car.image.length; i++) { %>
                                                        <div class="d-flex flex-column">
                                                            <img style="height: 10vh; width: 10vh;"
                                                                src="/<%= car.image[i].path %>" alt=""><a href="">Edit</a>
                                                        </div >
                                                        <% } %>
                                                    </td>
                                                    <td>
                                                        <%= car.name %>
                                                    </td>
                                                    <td>
                                                        <%= car.brand %>
                                                    </td>
                                                    <td>
                                                        <%= car.price %>
                                                    </td>
                                                    <td>
                                                        <%= car.ticket %>
                                                    </td>
                                                    <td>
                                                        <%= car.ticketrate %>
                                                    </td>
                                                    <td><a style="font-weight: bold;" class="btn"
                                                            href="/admin/editCar?id=<%= car._id%>">Edit</a>
                                                    
                                                        <a class="btn text-danger " data-toggle="modal"
                                                            data-target="#deleteModal<%= car._id%>"
                                                            style="font-weight: bold;">Delete</a>
                                                    </td>
                                                </tr>
                                                <!-- modal -->
                                                <div class="modal fade" id="deleteModal<%= car._id%>" tabindex="-1" role="dialog"
                                                    aria-labelledby="deleteModalLabel" aria-hidden="true">
                                                    <div class="modal-dialog" role="document">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                                <h5 class="modal-title" id="deleteModalLabel">Confirm
                                                                    Delete Action</h5>
                                                                <button type="button" class="close" data-dismiss="modal"
                                                                    aria-label="Close">
                                                                    <span aria-hidden="true">&times;</span>
                                                                </button>
                                                            </div>
                                                            <div class="modal-body">
                                                                Are you sure you want to delete this <%= car.name%>?
                                                            </div>
                                                            <div class="modal-footer">
                                                                <button type="button" class="btn btn-secondary"
                                                                    data-dismiss="modal">Cancel</button>

                                                                <form id="deleteForm"
                                                                    action="/admin/deleteCar/<%= car._id%>?_method=DELETE"
                                                                    method="POST">
                                                                    <input type="hidden" name="_method" value="DELETE">
                                                                    <button type="submit"
                                                                        class="btn btn-danger">Delete</button>
                                                                </form>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <%= i++%>
                                                    <% }) %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

